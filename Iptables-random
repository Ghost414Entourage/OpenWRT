# DROP rules for destination ports on br-lan
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 993 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 993 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 1900 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 1900 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 2350 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 2350 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 5222 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 5222 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 5223 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 5223 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 5229 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 5229 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 5353 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 5353 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 8001 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 8001 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 15600 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 15600 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 19302 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 19302 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 3478 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 3478 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p tcp --dport 3482 -j DROP
iptables -t raw -A PREROUTING -i br-lan -p udp --dport 3482 -j DROP
